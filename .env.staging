# Staging Environment Configuration
ENVIRONMENT=staging

# Database Configuration
DATABASE_URL=postgresql://staging_user:STAGING_PASSWORD@postgres-staging:5432/faceless_youtube_staging
MONGODB_URI=mongodb://staging_user:STAGING_PASSWORD@mongodb-staging:27017/faceless_youtube_staging
REDIS_URL=redis://redis-staging:6379/0

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://dashboard-staging:3000

# Frontend Configuration
REACT_APP_API_URL=http://api-staging:8000

# Logging
LOG_LEVEL=INFO
SENTRY_DSN=

# AI Configuration
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL=llama2

# YouTube Configuration
YOUTUBE_OAUTH_CREDENTIALS_PATH=/app/config/youtube_oauth.json

# Monitoring
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
